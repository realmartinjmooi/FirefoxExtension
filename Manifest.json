{
 
  "manifest_version": 2,
  "name": "YT Browse Time",
  "version": "2.0",
 
  "description": "Tracks your browsing time on any given domain",
 
  "background": {
    "scripts": ["background.js"]
  },
 
  "permissions": [
    "webNavigation",
    "storage"
  ],
 
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Browse Time",
    "default_popup": "view.html"
  }
 
}